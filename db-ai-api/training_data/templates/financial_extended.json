      "sql": "SELECT SUM(Sum) as TotalOutcome, COUNT(*) as PaymentCount FROM dbo.OutcomePaymentOrder WHERE Deleted = 0",
      "tables_used": ["OutcomePaymentOrder"]
    },
    {
      "id": "monthly_income_012",
      "category": "aggregation",
      "complexity": "medium",
      "question_en": "Monthly income totals",
