      "variations_uk": ["середній платіж"],
      "sql": "SELECT AVG(Sum) as AvgPayment, MIN(Sum) as MinPayment, MAX(Sum) as MaxPayment FROM dbo.IncomePaymentOrder WHERE Deleted = 0",
      "tables_used": ["IncomePaymentOrder"]
    },
    {
      "id": "client_payment_history_023",
      "category": "list",
      "complexity": "medium",
      "question_en": "Payment history for a client",
      "question_uk": "Історія платежів клієнта",
      "variations_en": ["client payments log", "client payment records"],
      "variations_uk": ["платежі клієнта", "історія оплат"],
      "sql": "SELECT ip.ID, ip.Created, ip.Sum, ip.Status FROM dbo.IncomePaymentOrder ip JOIN dbo.ClientAgreement ca ON ip.ClientAgreementID = ca.ID JOIN dbo.Client c ON ca.ClientID = c.ID WHERE ip.Deleted = 0 AND c.Name LIKE '%keyword%' ORDER BY ip.Created DESC",
      "tables_used": ["IncomePaymentOrder", "ClientAgreement", "Client"]
    },
    {
      "id": "net_cashflow_024",
      "category": "aggregation",
      "complexity": "complex",
      "question_en": "Net cashflow (income minus expenses)",
      "question_uk": "Чистий грошовий потік (надходження мінус витрати)",
      "variations_en": ["cashflow balance", "profit margin"],
      "variations_uk": ["баланс грошей", "різниця надходжень та витрат"],
      "sql": "SELECT (SELECT ISNULL(SUM(Sum), 0) FROM dbo.IncomePaymentOrder WHERE Deleted = 0) - (SELECT ISNULL(SUM(Sum), 0) FROM dbo.OutcomePaymentOrder WHERE Deleted = 0) as NetCashflow",
      "tables_used": ["IncomePaymentOrder", "OutcomePaymentOrder"]
    },
    {
      "id": "debt_aging_025",
      "category": "aggregation",
      "complexity": "complex",
      "question_en": "Debt aging report",
      "question_uk": "Звіт про старіння заборгованості",
      "variations_en": ["overdue debts", "debt by age"],
      "variations_uk": ["прострочені борги", "старі заборгованості"],
      "sql": "SELECT c.Name as ClientName, d.Sum, d.Created, DATEDIFF(day, d.Created, GETDATE()) as DaysOutstanding FROM dbo.Debt d JOIN dbo.ClientAgreement ca ON d.ClientAgreementID = ca.ID JOIN dbo.Client c ON ca.ClientID = c.ID WHERE d.Deleted = 0 AND d.Sum > 0 ORDER BY DaysOutstanding DESC",
      "tables_used": ["Debt", "ClientAgreement", "Client"]
    }
