      "tables_used": ["Agreement"]
    },
    {
      "id": "debt_by_client_008",
      "category": "aggregation",
      "complexity": "medium",
      "question_en": "Client debts total",
      "question_uk": "Заборгованість клієнтів",
      "variations_en": ["outstanding debts", "client balances", "money owed by clients"],
