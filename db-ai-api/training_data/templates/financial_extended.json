      "variations_uk": ["такс фрі по місяцях"],
      "sql": "SELECT YEAR(Created) as Year, MONTH(Created) as Month, COUNT(*) as OperationCount, SUM(Sum) as TotalSum FROM dbo.TaxFree WHERE Deleted = 0 GROUP BY YEAR(Created), MONTH(Created) ORDER BY Year DESC, Month DESC",
      "tables_used": ["TaxFree"]
    },
    {
      "id": "currency_count_018",
      "category": "aggregation",
      "complexity": "simple",
