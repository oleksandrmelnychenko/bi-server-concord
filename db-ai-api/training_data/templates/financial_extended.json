      "sql": "SELECT YEAR(Created) as Year, MONTH(Created) as Month, SUM(Sum) as MonthlyIncome, COUNT(*) as PaymentCount FROM dbo.IncomePaymentOrder WHERE Deleted = 0 GROUP BY YEAR(Created), MONTH(Created) ORDER BY Year DESC, Month DESC",
      "tables_used": ["IncomePaymentOrder"]
    },
