# Concord BI Server Configuration

# MSSQL Source Database
MSSQL_HOST=your-mssql-server
MSSQL_PORT=1433
MSSQL_DATABASE=YourDatabase
MSSQL_USER=datahub_user
MSSQL_PASSWORD=your-password

# PostgreSQL (Metadata Store)
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=concord_bi
POSTGRES_USER=concord
POSTGRES_PASSWORD=change-me-in-production

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# MinIO (Object Storage)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_ENDPOINT=minio:9000
MINIO_BUCKET=concord-data

# MLflow
MLFLOW_TRACKING_URI=http://mlflow:5000
MLFLOW_BACKEND_STORE_URI=postgresql://concord:change-me-in-production@postgres:5432/mlflow
MLFLOW_ARTIFACT_ROOT=s3://mlflow-artifacts

# DataHub
DATAHUB_GMS_HOST=datahub-gms
DATAHUB_GMS_PORT=8080

# API
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# LLM Configuration (for NL Queries)
LLM_MODEL=llama3.1:8b
LLM_ENDPOINT=http://ollama:11434
LLM_TEMPERATURE=0.1

# Monitoring
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin
